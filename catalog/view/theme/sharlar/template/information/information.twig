{{ header }}
<div id="information-information" class="container">
  <ul class="breadcrumbs">
    {% for breadcrumb in breadcrumbs %}
    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
    {% endfor %}
  </ul>
  <div class="row">{{ column_left }}
    {% if column_left and column_right %}
    {% set class = 'col-sm-6' %}
    {% elseif column_left or column_right %}
    {% set class = 'col-sm-9' %}
    {% else %}
    {% set class = 'col-sm-12' %}
    {% endif %}
    <div id="content" class="information-content white-background">{{ content_top }}
      <h1>{{ heading_title }}</h1>
      {{ description }}{{ content_bottom }}</div>
    {{ column_right }}</div>
</div>
  <section id="section-4">
            <div class="wrapper-map white-background brdr-20">
              <h1 class="cl-green">{{map_coordinates}}</h1>
                <div id="map" class="brdr-20"></div>
            </div>

        </section>
 <script src="catalog/view/theme/sharlar/js/google.map.js"></script>
 <script defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAnjyhj19PXFKy396KRoo1dV-kN8osQOlI&callback=initMap"
        defer></script>
{{ footer }}